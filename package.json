{
  "name": "kid-chameleon-browser",
  "version": "0.2.0",
  "description": "An browser for Kid Chameleon ROM assets",
  "author": "StealthC",
  "private": true,
  "engines": {
    "node": ">=22",
    "pnpm": ">=9.11.0"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "turbo build",
    "build-packages": "turbo build --filter='./packages/*'",
    "lint": "turbo lint --log-prefix=none --log-order=stream",
    "format": "turbo format",
    "dev": "turbo dev",
    "test": "pnpm vitest run",
    "prepare": "husky"
  },
  "packageManager": "pnpm@9.11.0",
  "devDependencies": {
    "husky": "^9.1.7",
    "rimraf": "^6.0.1",
    "turbo": "^2.3.4",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
}
